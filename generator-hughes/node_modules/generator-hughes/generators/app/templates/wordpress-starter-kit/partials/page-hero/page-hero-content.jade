.page-hero__content
	- while ( have_rows('hero_content') ) : the_row();
		- get_template_part('partials/page-hero/components/hero-heading');
		- get_template_part('partials/page-hero/components/hero-subheading');
		- get_template_part('partials/page-hero/components/hero-cta');
	- endwhile;
	
	- if(is_page(178)) : // If is thank you page
		.page-hero__content__social-follow
			- get_template_part('partials/social/follow-buttons');
	- endif;