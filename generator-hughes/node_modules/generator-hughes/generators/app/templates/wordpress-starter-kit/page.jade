- get_header();

- if( have_rows('Content') ):
  - while ( have_rows('Content') ) : the_row();
    - if( get_row_layout() == 'section' ):
        section.page-section
            - if( have_rows('section_content') ):
          
                - while ( have_rows('section_content') ) : the_row();
                  - if( get_row_layout() == 'page_hero' ):
                    - get_template_part('partials/page-hero/page-hero');
                  - endif;
                  - get_template_part('partials/content-block/content-block');
                  - get_template_part('partials/column-section/column-section');
                  - get_template_part('partials/contact-section/contact-section');
                  - get_template_part('partials/content-slider/content-slider');
         
                - endwhile;
             - endif;
    - endif;
  - endwhile;
- endif;

- get_footer();