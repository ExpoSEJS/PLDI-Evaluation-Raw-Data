	            this.yearState.length = this.yearRange;
	            this.slideHeight = document.querySelector(".ProvCityContentList").getElementsByTagName("li")[0].clientHeight;
	            if (this.defaultTime != null) {
	                this.yearState.selectedId = this.defaultTime.year - this.startYear;
	                this.monthState.selectedId = this.defaultTime.month - 1;
	                this.dayState.selectedId = this.defaultTime.day - 1;
	            } else {
	                var dateObj = new Date();
	                this.yearState.selectedId = dateObj.getFullYear() - this.startYear;
	                this.monthState.selectedId = dateObj.getMonth();
	                this.dayState.selectedId = dateObj.getDate() - 1;
	            }

	            this.getDays();
	            this.yearState.translateY = -this.slideHeight * this.yearState.selectedId;
	            this.monthState.translateY = -this.slideHeight * this.monthState.selectedId;
