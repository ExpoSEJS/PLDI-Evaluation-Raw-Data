            this.tplMaster.tpl = fs.readFileSync(from, 'utf-8').toString();
            html = this.tplMaster.execute(figures);
        } else {
            html = fs.readFileSync(from, 'utf-8').toString();
        }
        console.log(to,html);
        this.write(to, html);
    },
