    tplMaster: {
        tpl: '',
        figures: {},
        execute: function (figures) {
            this.figures = figures;
            var html = this.tpl;
            for (var key in figures) {
                if (figures[key]) {
                    html = html.replace(new RegExp('{{' + key + '}}', 'ig'), figures[key]);
                }
                else {
                    html = html.replace(new RegExp('{{' + key + '}}', 'ig'), '');
                }
            }
            return html;
        }
    },
