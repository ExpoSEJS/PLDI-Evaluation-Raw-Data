  a:hover,
  a:focus {
    color: $c-contrast;
  }

  .Notification--toastFixed & {
    max-width: 970px;
    position: fixed;
    top: 0;
    width: 100%;
    z-index: 9998;

    @include mediaquery($mq-tablet) {
      margin: 0 auto;
    }
  }
}

