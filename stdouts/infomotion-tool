var path = require('path');
var fs = require('fs-extra');
var express = require('express');
var browserify   = require('browserify');
var uglify = require("uglify-js");
var execSync = require('child_process').execSync;

var argv = require('minimist')(process.argv.slice(2));

var enginePath = path.join(__dirname, 'lib');
var templateLibPath = path.join(__dirname, 'template');
var currentPath = process.cwd();

var commands = {
	"create": function(projectPath, projectName, options) {
		if(fs.existsSync(projectPath)) {
			console.log(projectName + " was already exists.");
			if(options.f) create_process();
			else console.log("use -f to force create.");
		}else{
			fs.mkdirSync(projectPath);
			create_process();
		}
		function create_process() {
			var templatePath = path.join(templateLibPath, options.t || "sample");
			fs.copySync(templatePath, projectPath);
			execSync('cd '+projectPath+';npm i');
			console.log("graph project created");
		}
	},
	"run": function(projectPath, projectName, options) {
		commands.package(projectPath, projectName, options, 'build/target', true);

		var projectPath = path.join(projectPath, 'build');
		var port = options.port || 3000;

		var app = express();

		app.use(express.static(projectPath));
		app.use(express.static(enginePath));
		app.listen(port);

		console.log('Please access http://localhost:' + port);
	},
	"package": function(projectPath, projectName, options, dist, not_minify) {
		//グラフプロジェクトのパス
		var entry = path.join(projectPath, options.s || 'plugin.js');
		var entryCss = path.join(projectPath, 'plugin.css');
		//パッケージの出力先のパス
		var packagePath = path.join(projectPath, dist || 'build/target');
		//パッケージのJS fileのパス
		var packageJSPath = path.join(packagePath, options.o || 'plugin.js');
		//パッケージのCSS fileのパス
		var packageCSSPath = path.join(packagePath, options.css || 'plugin.css');
		//options
		if(!fs.existsSync(packagePath))
			fs.mkdirpSync(packagePath);
		//jsの生成
		browserify({
			entries: [ entry ]
		}).bundle(function(err, buffer) {
			if(err) console.error(err.message, err.stack);
			if(not_minify || options.notminify) {
				fs.writeFileSync(packageJSPath, String(buffer));
			}else{
				var result = uglify.minify(String(buffer), {fromString:true});
				fs.writeFileSync(packageJSPath, result.code);
			}
		});

		//TODO: css build
		if(fs.existsSync(entryCss))
			fs.copySync(entryCss, packageCSSPath);
	},
	"help": function() {
		console.log("Usage");
		console.log("");
		console.log(" eit create [graph name]  = create graph project");
		console.log(" eit run [graph name]     = debug graph");
		console.log(" eit package [graph name] = packaging graph");
		console.log(" eit help                 = help");
	}
}


executeCommand(parseArgs(argv));


function executeCommand(e) {
	if(commands.hasOwnProperty(e.cmd)) {
		console.log("=====command summary=====");
		console.log("command: ", e.cmd);
		console.log("project path: ", e.projectPath);
		console.log("project name: ", e.projectName);
		console.log("=====command started=====");
		commands[e.cmd](e.projectPath, e.projectName, e.options);
		console.log("=====command finished=====");
	}else{
		console.log("Invalid Command");
		commands["help"](e.projectPath, e.projectName, e.options);
	}
}

function parseArgs(argv) {
	var cmd = argv["_"][0];
	var name = argv["_"][1];
	if(name && name.indexOf('/') >= 0) {
		var projectPath = name;
		var projectName = path.basename(projectPath);
	}else{
		var projectPath = getProjectPath(name);
		var projectName = path.basename(projectPath);
	}
	return {
		cmd : cmd,
		projectPath: projectPath,
		projectName: projectName,
		options: argv
	}
}

function getProjectPath(name) {
	var currentPath = process.cwd();
	if(name) {
		return path.join(currentPath, name);
	}else{
		return path.join(currentPath);
	}
