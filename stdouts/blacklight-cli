var child_process=require("child_process");
var docopt = require("docopt").docopt;
var finder=require("./libs/blacklight-finder");
var c=require("./libs/colors");
var readlineSync = require('readline-sync');
var fs=require("fs"), _path=require("path");
var fileExists=require("file-exists");
var package=require("./package.json");

var version=process.version.match(/^v(\d*)\./);
if(!version || parseInt(version[1])<4){
	console.error(c.green("\nThe 'bl' command requires Node version 4 or higher.\nPlease upgrade.") );
	process.exit(1);
}


var usage=require("./bl-usage-message");


finder.blacklightRoot(function(err,blacklightRoot){

	var args = docopt(usage, {version: "Blacklight CLI\n" + package.version, options_first: true});

	var command = args["<command>"] ;

	var argv = [command] + args["<args>"];
	var commands = ["init","module", "site", "git", "sling", "version", "start", "test", "config"], rootless=["init", "module"];

	var dottedCommand=command.match(/\./);

	function notBLfolderError(){console.error("bl command '" + command + "' requires an existing Blacklight project, but none found from this folder."); process.exit(1);}


	if(commands.indexOf(command)>-1 || dottedCommand){
		if(!blacklightRoot){
			if(dottedCommand){unknownCommand(command);return;}

			if(rootless.indexOf(command)<0){
				notBLfolderError();
			}else{
				if(command==="module" && args["<args>"][0] ==="install"){
					console.log(c.red("PROBLEM:"), "No 'blacklight_modules' folder found.");
					var answer=readlineSync.question(c.w.white("Would you like to create 'blacklight_modules' here? ") + c.grey("(Y|n) ") );
					answer=answer?answer[0].toLowerCase():"y";
					if(answer==="n"){
						console.log(c.grey("\nNot making 'blacklight_modules' folder.\nExiting with no changes."));
					}else{
						fs.mkdirSync("blacklight_modules");
						console.log(c.green("\nCreated blacklight_modules folder.\nPlease try ") + c.white("bl module install") + c.green(" again."));
					}
					return;
				}else{
					if(command!=="init"){
						notBLfolderError();
					}
				}
			}
		}


		var jsFile;

		if(dottedCommand){
			jsFile = _path.join(blacklightRoot, "apps", command.replace(/\./,"/"), "cli.js" );
			if(!fileExists(jsFile)){
				console.error(c.red("ERROR:"), "Module-specific CLI directive does not exist:\n", c.white(jsFile) );
				process.exit(1);
			}
		}else{
			jsFile =__dirname + "/tools/" + command + "/bl-" + command + ".js";
		}

		var childArgs = [jsFile, command].concat(args["<args>"]);

		var envCopy={};
		for(var key in process.env) {
			envCopy[key] = process.env[key];
		}

		if(blacklightRoot){
			envCopy.NODE_CONFIG_DIR = blacklightRoot + "/config";
			envCopy.BLACKLIGHT_ROOT = blacklightRoot;
		}


		var child = child_process.spawn(process.execPath, childArgs, {env: envCopy, stdio: "inherit"});
		child.on("close", function(code){
			process.exit(code);
		});

	}
	else{
		unknownCommand(command);
	}


	function unknownCommand(command){
		console.error("'" + command + "' is not a bl command. See 'bl --help' for available commands.");
	}
});


