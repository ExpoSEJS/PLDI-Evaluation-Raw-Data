var forever = require('forever')
	, commander = require('commander');

//start 명령에 대해 처리하는 부분입니다.
commander
	.command('start [option]')
	.option('-d, --daemon', 'run the server as a daemon using the forever module...')
	.action(function (env, options) {
		if (options.daemon) { //데몬 옵션이 있을 때 처리하는 부분입니다.
			//데몬 형식으로 실행
			forever.startDaemon(__dirname+'/app.js', {
				'env': { 'NODE_ENV': 'production' },
				'spawnWith': { env: process.env }
			});
			console.log("Realtime Editor server is started...");
		}
		else {
			//데몬 형식이 아닌 forever 모듈을 이용하여 실행합니다.
			forever.start(__dirname+'/app.js', []);
		}
	});

//restart 명령에 대해 처리하는 부분입니다.
commander
	.command('restart')
	.action(function (env, options) {
		//종료하고 새로 시작하는 부분입니다.
		forever.stop(__dirname+'/app.js', stop);
		//restart는 데몬으로 실행됐을 경우에만 호출가능한 것이기 때문에 바로 데몬으로 실행합니다.
		forever.startDaemon(__dirname+'/app.js', {
			'env': { 'NODE_ENV': 'production' },
			'spawnWith': { env: process.env }
		});
		
		console.log("Realtime Editor server is restarted...");
	});

//stop 명령에 대해 처리하는 부분입니다.
commander
	.command('stop')
	.action(function (env, options) {
		//stopAll()을 통해 forever 로 실행된 모든 애플리케이션을 종료하도록 했습니다.
		//forever.stopAll();
		//만약 하나만 종료하고 싶다면 아래와 같이 작성하면 됩니다.
		//forever.stop("실행 파일의 경로", true);
		forever.stop(__dirname+'/app.js', true);
		
		console.log("Realtime Editor server is stopped...");
	});

//process.argv 정보를 파싱해서 위에서 정의했던 명령들 중 하나가 실행될 수 있도록 합니다.
