	  havingWrapped: function havingWrapped(callback) {
	    this._statements.push({
	      grouping: 'having',
	      type: 'havingWrapped',
	      value: callback,
	      bool: this._bool(),
	      not: this._not()
	    });
	    return this;
	  },
