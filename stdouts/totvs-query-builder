	      ret = arguments[1];
	    }
	    if (!(0, _isEmpty3.default)(ret)) this.returning(ret);
	    this._single.update = obj;
	    return this;
	  },


	  // Sets the returning value for the query.
	  returning: function returning(_returning) {
	    this._single.returning = _returning;
	    return this;
	  },


	  // Delete
