	var charsRegex = exports.charsRegex = /[\0\b\t\n\r\x1a\"\'\\]/g; // eslint-disable-line no-control-regex
	var charsMap = exports.charsMap = {
	  '\0': '\\0',
	  '\b': '\\b',
