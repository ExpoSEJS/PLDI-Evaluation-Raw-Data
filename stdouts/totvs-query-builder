	    queryBuilderService.$inject = [];
	    function queryBuilderService() {
	        var service = this;
	        var _query;

	        service.select = function(columns) {
	            _query = knex.select(columns);
	            return service;
	        };

	        service.insert = function(data) {
	            _query = knex.insert(data);
	            return service;
	        };

	        service.update = function(table, data) {
	            _query = knex(table).update(data);
	            return service;
	        };

	        service.delete = function() {
	            _query = knex.del();
	            return service;
	        };
	        service.del = service.delete;

	        service.from = function(table) {
	            _query.from(table);
	            return service;
	        };

	        service.into = function(table) {
	            _query.into(table);
	            return service;
	        };

	        service.column = function(columns) {
	            _query.column(columns);
	            return service;
	        };

	        service.withSchema = function(schemaName) {
	            _query.withSchema(schemaName);
	            return service;
	        };

	        service.where = function(filters, operator, value) {
	            if(angular.isFunction(filters)) {
	                _query.where(filters);
	                return service;
	            }

	            var criteria = _parseFilters(filters, operator, value);
	            criteria.forEach(function(filter) {
	                if(filter.key && (filter.value !== undefined && filter.value !== null) && filter.operator) {
	                    _query.where(filter.key, filter.operator, filter.value);
	                } else {
	                    _query.where(filter);
	                }
	            });
	            return service;
	        };

	        service.andWhere = function(filters, operator, value) {
	            if(angular.isFunction(filters)) {
	                _query.andWhere(filters);
	                return service;
	            }

	            var criteria = _parseFilters(filters, operator, value);
	            criteria.forEach(function(filter) {
	                if(filter.key && (filter.value !== undefined && filter.value !== null) && filter.operator) {
	                    _query.andWhere(filter.key, filter.operator, filter.value);
	                } else {
	                    _query.andWhere(filter);
	                }
	            });

	            return service;
	        };

	        service.orWhere = function(filters, operator, value) {
	            if(angular.isFunction(filters)) {
	                _query.orWhere(filters);
	                return service;
	            }

	            var criteria = _parseFilters(filters, operator, value);
	            criteria.forEach(function(filter) {
	                if(filter.key && (filter.value !== undefined && filter.value !== null) && filter.operator) {
	                    _query.orWhere(filter.key, filter.operator, filter.value);
	                } else {
	                    _query.orWhere(filter);
	                }
	            });
	            return service;
	        };

	        service.whereNot = function(filters, operator, value) {
	            if(angular.isFunction(filters)) {
	                _query.whereNot(filters);
	                return service;
