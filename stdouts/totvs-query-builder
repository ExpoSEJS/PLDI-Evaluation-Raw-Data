	        /* CUSTOM METHODS */
	        service.like = function(column, value, type) {
	            _query.where(column, 'LIKE', _setLikeValue(value, type));
	            return service;
	        };
	        service.andLike = service.like;

	        service.orLike = function(column, value, type) {
	            _query.orWhere(column, 'LIKE', _setLikeValue(value, type));
	            return service;
	        };

	        service.notLike = function(column, value, type) {
	            _query.whereNot(column, 'LIKE', _setLikeValue(value, type));
	            return service;
	        };

	        service.orNotLike = function(column, value, type) {
	            _query.orWhereNot(column, 'LIKE', _setLikeValue(value, type));
	            return service;
	        };

	        service.get = function() {
	            return _query;
	        };

	        service.toSqlite = function(query) {
	            if(angular.isDefined(query)) {
	                if(!angular.isString(query)) {
	                    query = query.toString();
	                }
	            } else {
	                query = service.get().toString();
	            }

	            query = query.replace(new RegExp('\\btrue\\b', 'g'), 1);
	            query = query.replace(new RegExp('\\bfalse\\b', 'g'), 0);

	            return query;
	        };

	        /* API PRIVATE METHODS */
	        function _parseFilters(filters, operator, value) {
	            var criteria = [];

	            if((operator !== undefined && value !== null) && (value !== undefined && value !== null)) {
	                criteria.push({
	                    'key': filters,
	                    'value': value,
	                    'operator': operator
	                });
	                return criteria;
	            } else {
	                value = operator;
	            }

	            if(angular.isArray(filters)) {
	                filters.forEach(function(filter) {
	                    if(filter.key && (filter.value !== undefined && filter.value !== null)) {
	                        criteria.push({
	                            'key': filter.key,
	                            'value': filter.value,
	                            'operator': filter.operator || '='
	                        });
	                    } else {
	                        for (var key in filter) {
	                            criteria.push({
	                                'key': key,
	                                'value': filter[key],
	                                'operator': '='
	                            });
	                        }
	                    }
	                });
	            } else if(angular.isString(filters)) {
	                var filter = {};
	                filter[filters] = value;
	                criteria.push(filter);
	            } else if(angular.isObject(filters)) {
	                criteria.push(filters);
	            }
	            return criteria;
	        }

	        function _setLikeValue(value, type) {
	            switch (type) {
	                case 'begins':
	                value = value + '%';
	                break;
	                case 'ends':
	                value = '%' + value;
	                break;
	                case 'contains':
	                value = '%' + value + '%';
	                break;
	                default:
	                value = '%' + value + '%';
	                break;
	            }
	            return value;
	        }

	        return service;
	    }

	    angular.module('totvs.query.builder', [])
	    .service('totvs.query.builder', queryBuilderService);
	})(window, window.angular);


