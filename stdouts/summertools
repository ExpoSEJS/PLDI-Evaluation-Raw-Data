var fs = require('fs');
var program = require('commander');
program
.version(require('../package.json').version)
.usage('[options] [dir|file ...]')
.option('-c, --create', 'create a summer project!')

program.on('create', function(){
	console.log('begin create');
	var inquirer = require("inquirer");
	var path = require('path');
	// current path
	var dir = process.cwd()
	// ask name
	inquirer.prompt([{
		message:'name:',
		name: 'name',
		default: path.basename(process.cwd())}
		], function( answers ) {
		var vfs = require('vinyl-fs');
		vfs.src([__dirname+"/../templates/normal/**"])
		  .pipe(vfs.dest(dir))
		  .on('end', function(){
			  var pack = path.normalize(path.join(dir,'package.json'));
			  fs.readFile(pack, 'utf-8', function(err, data){
				  data = data.replace('$name$',answers.name);
				  fs.writeFile(pack, data, function(){
					//   console.log('begin install node_modules')
					//   var exec = require('child_process').exec;
					//   exec("npm i", function(err,stdout,stderr){
					// 	  if(err) {
					// 		console.log('get weather api error:'+stderr);
					// 	  } else {
					// 		console.log(stdout);
					// 		console.log('end create: sucess!');
					// 	  }
					// });
                    console.log('end create: sucess!');
				  })
			  })
		  });
	});
});

