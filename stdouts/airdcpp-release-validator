	var currentCallbackId = 0;

	// Internal

	// This creates a new callback ID for a request
	var getCallbackId = function getCallbackId() {
		if (currentCallbackId > 100000) {
			currentCallbackId = 0;
		}

		currentCallbackId += 1;
		return currentCallbackId;
	};

	var filterPassword = function filterPassword(data) {
		if (!data.hasOwnProperty('password')) {
			return data;
		}

		return _extends({}, data, {
			password: '(hidden)'
		});
	};

	var sendRequest = function sendRequest(path, data, method, authenticating) {
		// Pre-checks
		if (!authenticating && !socket.isConnected()) {
			logger.warn('Attempting to send request on a non-authenticated socket: ' + path);
			return _Promise2.default.reject('Not authorized');
		}

		var callbackId = getCallbackId();

		// Reporting
		(0, _invariant2.default)(path, 'Attempting socket request without a path');

		var ignored = (0, _utils.eventIgnored)(path, options.ignoredRequestPaths);
		if (!ignored) {
			logger.verbose(_chalk2.default.white.bold(callbackId), method, path, data ? filterPassword(data) : '(no data)');
		}

		// Callback
		var resolver = _Promise2.default.pending();

		callbacks[callbackId] = {
			time: new Date().getTime(),
			resolver: resolver,
			ignored: ignored
		};

		// Actual request
		var request = {
			path: path,
			method: method,
			data: data,
			callback_id: callbackId
		};

		socket.nativeSocket.send(JSON.stringify(request));
		return resolver.promise;
	};

	// Report timed out requests
	// This is more about spotting backend issues, such as frozen threads and dropped responses
	// The socket itself should handle actual connection issues
	var reportTimeouts = function reportTimeouts() {
		var now = new Date().getTime();
		Object.keys(callbacks).forEach(function (callbackId) {
			var request = callbacks[callbackId];
			if (request.time + options.requestTimeout * 1000 < now) {
				logger.warn('Request ' + callbackId + ' timed out');
			}
		});
	};

	var timeoutReportInterval = setInterval(reportTimeouts, 30000);

	socket.reportRequestTimeouts = reportTimeouts; // for testing

	// Public
	socket.put = function (path, data) {
		return sendRequest(path, data, 'PUT');
	};

	socket.patch = function (path, data) {
		return sendRequest(path, data, 'PATCH');
	};

	socket.post = function (path, data) {
		return sendRequest(path, data, 'POST');
	};

	socket.delete = function (path, data) {
		(0, _invariant2.default)(!data, 'No data is allowed for delete command');
		return sendRequest(path, null, 'DELETE');
	};

	socket.get = function (path, data) {
		(0, _invariant2.default)(!data, 'No data is allowed for get command');
		return sendRequest(path, null, 'GET');
	};

