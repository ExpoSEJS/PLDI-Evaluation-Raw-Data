var _ApiConstants = __webpack_require__(107);

var _ApiConstants2 = _interopRequireDefault(_ApiConstants);

var _SocketLogger = __webpack_require__(108);

var _SocketLogger2 = _interopRequireDefault(_SocketLogger);

var _SocketSubscriptionHandler = __webpack_require__(116);

var _SocketSubscriptionHandler2 = _interopRequireDefault(_SocketSubscriptionHandler);

var _SocketRequestHandler = __webpack_require__(117);

var _SocketRequestHandler2 = _interopRequireDefault(_SocketRequestHandler);

var _invariant = __webpack_require__(20);

var _invariant2 = _interopRequireDefault(_invariant);

var _Promise = __webpack_require__(61);

var _Promise2 = _interopRequireDefault(_Promise);

function _interopRequireDefault(obj) { return obj && obj.__esModule ? obj : { default: obj }; }

var defaultOptions = {
	autoReconnect: true,
	reconnectInterval: 10,
	userSession: false,
	requestTimeout: 30
};

var ApiSocket = function ApiSocket(userOptions, WebSocketImpl) {
	var options = _extends({}, defaultOptions, userOptions);

	var subscriptions = null;
	var requests = null;

	var ws = null;
	var authToken = null;

	var socket = null;
	var reconnectTimer = null;
	var disconnected = true;

	var connectedCallback = null;
	var sessionResetCallback = null;
	var disconnectedCallback = null;

	var logger = (0, _SocketLogger2.default)(options);

	(0, _invariant2.default)(userOptions.url, '"url" must be defined in settings object');

	var resetSession = function resetSession() {
		if (authToken) {
			if (sessionResetCallback) {
				sessionResetCallback();
			}

			authToken = null;
		}
	};

	var onClosed = function onClosed(event) {
		logger.info(event.reason ? 'Websocket was closed: ' + event.reason : 'Websocket was closed');

		requests.onSocketDisconnected();
		subscriptions.onSocketDisconnected();
		ws = null;

		if (disconnectedCallback) {
			disconnectedCallback(event.reason, event.code);
		}

		if (authToken && options.autoReconnect && !disconnected) {
			setTimeout(function (_) {
				socket.reconnect().catch(function (error) {
					return logger.error('Reconnect failed for a closed socket', error.message);
				});
			});
		}
	};

	var onMessage = function onMessage(event) {
		var messageObj = JSON.parse(event.data);
		if (messageObj.callback_id) {
			// Callback
			requests.handleMessage(messageObj);
		} else {
			// Listener message
			subscriptions.handleMessage(messageObj);
		}
	};

	var setSocketHandlers = function setSocketHandlers() {
		subscriptions = (0, _SocketSubscriptionHandler2.default)(socket, logger, options);
		requests = (0, _SocketRequestHandler2.default)(socket, logger, options);

		ws.onerror = function (event) {
			logger.error('Websocket failed: ' + event.reason);
		};

		ws.onclose = onClosed;
		ws.onmessage = onMessage;
	};

	// Connect handler for creation of new session
	var handleLogin = function handleLogin() {
		var username = arguments.length > 0 && arguments[0] !== undefined ? arguments[0] : options.username;
		var password = arguments.length > 1 && arguments[1] !== undefined ? arguments[1] : options.password;

		return requests.postAuthenticate(_ApiConstants2.default.LOGIN_URL, {
			username: username,
			password: password,
			user_session: options.userSession
		}, true);
	};

	// Connect handler for associating socket with an existing session token
	var handleAuthorizeToken = function handleAuthorizeToken() {
		return requests.postAuthenticate(_ApiConstants2.default.CONNECT_URL, {
			auth_token: authToken
		}, true);
	};

	var authenticate = function authenticate(resolve, reject, authenticationHandler, reconnectHandler) {
		authenticationHandler().then(function (data) {
			// Authentication succeed

			if (!authToken) {
				// New session
				logger.info('Login succeed');
				authToken = data.auth_token;
			} else {
				// Existing session
				logger.info('Socket associated with an existing session');
			}

			if (connectedCallback) {
				// Catch separately as we don't want an infinite reconnect loop
				try {
					connectedCallback(data);
				} catch (e) {
					console.error('Error in socket connect handler', e.message);
				}
			}

			resolve(data);
		}).catch(function (error) {
			if (error.code) {
				if (authToken && error.code === 400 && options.autoReconnect) {
					// The session was lost (most likely the client was restarted)
					logger.info('Session lost, re-sending credentials');
					resetSession();

					authenticate(resolve, reject, handleLogin);
					return;
				} else if (error.code === 401) {
					// Invalid credentials, reset the token if we were reconnecting to avoid an infinite loop
					resetSession();
				}

				// Authentication was rejected
				socket.disconnect();
			} else {
				// Socket was disconnected during the authentication
				logger.info('Socket disconnected during authentication, reconnecting');
				reconnectHandler();
				return;
			}

			reject(error);
		});
	};

	var connectInternal = function connectInternal(resolve, reject, authenticationHandler) {
		var reconnectOnFailure = arguments.length > 3 && arguments[3] !== undefined ? arguments[3] : true;

		ws = new WebSocketImpl(options.url);

		var scheduleReconnect = function scheduleReconnect() {
			ws = null;
			if (!reconnectOnFailure) {
				reject('Cannot connect to the server');
				return;
			}

			reconnectTimer = setTimeout(function () {
				logger.info('Socket reconnecting');
				connectInternal(resolve, reject, authenticationHandler, reconnectOnFailure);
			}, options.reconnectInterval * 1000);
		};

		ws.onopen = function () {
			logger.info('Socket connected');

			setSocketHandlers();
			authenticate(resolve, reject, authenticationHandler, scheduleReconnect);
		};

		ws.onerror = function (event) {
			logger.error('Connecting socket failed');
			scheduleReconnect();
		};
	};

	var startConnect = function startConnect(authenticationHandler, reconnectOnFailure) {
		disconnected = false;
		return new _Promise2.default(function (resolve, reject) {
			logger.info('Starting socket connect');
			connectInternal(resolve, reject, authenticationHandler, reconnectOnFailure);
		});
	};

	// Is the socket connected and authorized?
	var isConnected = function isConnected() {
		return !!(ws && ws.readyState === (ws.OPEN || 1) && authToken);
	};

	// Is the socket connected but not possibly authorized?
	var isConnecting = function isConnecting() {
		return !!(ws && !isConnected());
	};

	var isActive = function isActive() {
		return !!ws;
	};

	// Disconnects the socket but keeps the session token
	var disconnect = function disconnect() {
		var autoConnect = arguments.length > 0 && arguments[0] !== undefined ? arguments[0] : false;

		if (!ws) {
			return;
		}

		disconnected = !autoConnect;
		logger.info('Disconnecting socket');
		clearTimeout(reconnectTimer);

		ws.close();
	};

	socket = {
		get nativeSocket() {
			return ws;
		},

		connect: function connect(username, password) {
			var reconnectOnFailure = arguments.length > 2 && arguments[2] !== undefined ? arguments[2] : true;

			if (isActive()) {
				throw 'Connect may only be used for a closed socket';
			}

			resetSession();

			return startConnect(function () {
				return handleLogin(username, password);
			}, reconnectOnFailure);
		},


		// Connect and attempt to associate the socket with an existing session
		reconnect: function reconnect(token) {
			var reconnectOnFailure = arguments.length > 1 && arguments[1] !== undefined ? arguments[1] : true;

			if (isActive()) {
				throw 'Reconnect may only be used for a closed socket';
			}

			if (token) {
				authToken = token;
			}

			if (!authToken) {
				throw 'No session token available for reconnecting';
			}

			logger.info('Reconnecting socket');

			return startConnect(handleAuthorizeToken, reconnectOnFailure);
		},


		// Remove the associated API session and close the socket
		logout: function logout() {
			var resolver = _Promise2.default.pending();
			socket.delete(_ApiConstants2.default.LOGOUT_URL).then(function (data) {
				logger.info('Logout succeed');
				resetSession();

				resolver.resolve(data);

				// Don't fire the disconnected event before resolver actions are handled
				disconnect();
			}).catch(function (error) {
				logger.error('Logout failed', error);
				resolver.reject(error);
			});

			return resolver.promise;
		},


		// Function to call each time the socket has been connected (and authorized)
		set onConnected(handler) {
			connectedCallback = handler;
		},

		// Function to call each time the stored session token was reset (manual logout/rejected reconnect)
		set onSessionReset(handler) {
			sessionResetCallback = handler;
		},

		// Function to call each time the socket has been disconnected
		set onDisconnected(handler) {
			disconnectedCallback = handler;
		},

		disconnect: disconnect,
		isConnecting: isConnecting,
		isConnected: isConnected,
		isActive: isActive,
		logger: logger
	};

	return socket;
};

