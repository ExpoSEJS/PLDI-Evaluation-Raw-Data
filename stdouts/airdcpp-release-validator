	value: true
});

var _invariant = __webpack_require__(20);

var _invariant2 = _interopRequireDefault(_invariant);

var _events = __webpack_require__(6);

var _utils = __webpack_require__(60);

function _interopRequireDefault(obj) { return obj && obj.__esModule ? obj : { default: obj }; }

var SocketSubscriptionHandler = function SocketSubscriptionHandler(socket, logger, _ref) {
	var _ref$ignoredListenerE = _ref.ignoredListenerEvents,
	    ignoredListenerEvents = _ref$ignoredListenerE === undefined ? [] : _ref$ignoredListenerE;

	// Internal
	var getEmitId = function getEmitId(event, id) {
		(0, _invariant2.default)(id !== 0, 'Entity ID "0" is not allowed');
		return id ? event + id : event;
	};

	var getSubscriptionUrl = function getSubscriptionUrl(moduleUrl, id, event) {
		if (id) {
			return moduleUrl + '/' + id + '/listeners/' + event;
		}

		return moduleUrl + '/listeners/' + event;
	};

	var subscriptions = {};
	var emitter = new _events.EventEmitter();

	// Subscriptions pending to be added in the API
	var pendingSubscriptions = {};

	var removeSocketListener = function removeSocketListener(subscriptionUrl, subscriptionId, callback, sendApi) {
		if (!socket.isConnected()) {
			return;
		}

		subscriptions[subscriptionId]--;
		emitter.removeListener(subscriptionId, callback);

		if (subscriptions[subscriptionId] === 0) {
			if (sendApi && socket.isConnected()) {
				socket.delete(subscriptionUrl).catch(function (error) {
					return logger.error('Failed to remove socket listener', subscriptionUrl, error);
				});
			}

			delete subscriptions[subscriptionId];
		}
	};

	var removeLocalListener = function removeLocalListener(subscriptionId, callback) {
		emitter.removeListener(subscriptionId, callback);
	};

	var handleHookAction = function handleHookAction(apiModuleUrl, callback, data, completionId) {
		//const completionUrl = apiModuleUrl + '/' + completionId;
		callback(data, function (completionData) {
			socket.post(apiModuleUrl + '/' + completionId + '/resolve', completionData).catch(function (error) {
				return logger.error('Failed to complete hook action', apiModuleUrl, error);
			});
		}, function (rejectId, rejectMessage) {
			socket.post(apiModuleUrl + '/' + completionId + '/reject', {
				reject_id: rejectId,
				message: rejectMessage
			}).catch(function (error) {
				return logger.error('Failed to complete failed hook action', apiModuleUrl, error);
			});
		});
	};

	var onSubscriptionAddSucceeded = function onSubscriptionAddSucceeded(subscriptionId) {
		var pending = pendingSubscriptions[subscriptionId];
		pending.forEach(function (pendingItem) {
			return pendingItem.resolver.resolve(pendingItem.removeHandler);
		});

		subscriptions[subscriptionId] = pending.length;
		delete pendingSubscriptions[subscriptionId];
	};

	var onSubscriptionAddFailed = function onSubscriptionAddFailed(subscriptionId, error) {
		var pending = pendingSubscriptions[subscriptionId];
		pending.forEach(function (pendingItem) {
			return pendingItem.resolver.reject(error);
		});

		delete pendingSubscriptions[subscriptionId];
	};

	var addPendingEntry = function addPendingEntry(subscriptionUrl, subscriptionId, callback, data) {
		var removeHandler = function removeHandler() {
			var sendApi = arguments.length > 0 && arguments[0] !== undefined ? arguments[0] : true;
			return removeSocketListener(subscriptionUrl, subscriptionId, callback, sendApi);
		};

		if (!subscriptions[subscriptionId]) {
			if (!pendingSubscriptions[subscriptionId]) {
				pendingSubscriptions[subscriptionId] = [];
				socket.post(subscriptionUrl, data).then(onSubscriptionAddSucceeded.bind(undefined, subscriptionId), onSubscriptionAddFailed.bind(undefined, subscriptionId));
			}

			var resolver = Promise.pending();
			pendingSubscriptions[subscriptionId].push({
				resolver: resolver,
				removeHandler: removeHandler
			});

			return resolver.promise;
		}

		subscriptions[subscriptionId]++;
		return Promise.resolve(removeHandler);
	};

	// Public

	// Listen to a specific event without sending subscription to the server
	socket.addViewUpdateListener = function (viewName, callback, id) {
		var subscriptionId = getEmitId(viewName + '_updated', id);
		emitter.on(subscriptionId, callback);
		return function () {
			return removeLocalListener(subscriptionId, callback);
		};
	};

	// Listen to a specific event and manage the API subscription automatically
	socket.addListener = function (apiModuleUrl, event, callback, entityId) {
		if (!socket.isConnected()) {
			throw 'Listeners can be added only for a connected socket';
		}

		var subscriptionId = getEmitId(event, entityId);
		var subscriptionUrl = getSubscriptionUrl(apiModuleUrl, entityId, event);

		emitter.on(subscriptionId, callback);
		return addPendingEntry(subscriptionUrl, subscriptionId, callback);
	};

	socket.hasListeners = function () {
		return emitter.listenerCount > 0 || Object.keys(subscriptions).length > 0;
	};

	socket.addHook = function (apiModuleUrl, event, callback, subscriberInfo) {
		if (!socket.isConnected()) {
			throw 'Hooks can be added only for a connected socket';
		}

		var subscriptionId = event;
		if (subscriptions[subscriptionId] || pendingSubscriptions[subscriptionId]) {
			throw 'Hook exists';
		}

		var subscriptionUrl = apiModuleUrl + '/hooks/' + event;

		callback = handleHookAction.bind(undefined, subscriptionUrl, callback);
		emitter.on(subscriptionId, callback);

		return addPendingEntry(subscriptionUrl, subscriptionId, callback, subscriberInfo);
	};

	socket.getPendingSubscriptionCount = function () {
		return Object.keys(pendingSubscriptions).length;
	};

	// For the socket
	var Handler = {
		onSocketDisconnected: function onSocketDisconnected() {
			emitter.removeAllListeners();
			subscriptions = {};
		},
		handleMessage: function handleMessage(message) {
			var ignored = (0, _utils.eventIgnored)(message.event, ignoredListenerEvents);
			if (message.completion_id) {
				if (!ignored) {
					logger.verbose(message.event, '(completion id ' + message.completion_id + ')', message.data);
				}

				emitter.emit(message.event, message.data, message.completion_id);
			} else {
				if (!ignored) {
					logger.verbose(message.event, message.id ? '(entity ' + message.id + ')' : '(no entity)', message.data);
				}

				if (message.id) {
					// There can be subscribers for a single entity or for all events of this type... emit for both
					emitter.emit(getEmitId(message.event, message.id), message.data, message.id);
				}

				emitter.emit(message.event, message.data, message.id);
			}
		}
	};

	return Handler;
};

exports.default = SocketSubscriptionHandler;

