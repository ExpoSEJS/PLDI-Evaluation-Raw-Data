/***/ (function(module, exports, __webpack_require__) {

"use strict";


var simpleReleaseReg = /^[A-Z0-9]\S{3,}-[A-Za-z0-9_]{2,}$/; // case sensitive

var isReleaseName = function isReleaseName(name) {
	return simpleReleaseReg.test(name);
};

module.exports = {
	isReleaseName: isReleaseName
};

