function _interopRequireDefault(obj) { return obj && obj.__esModule ? obj : { default: obj }; }

var ValidatorErrorReporter = exports.ValidatorErrorReporter = function ValidatorErrorReporter(directoryInfo, totalErrors, logger) {
	var validatorErrors = {};

	var reduceMessage = function reduceMessage(reducedText, fileName, index) {
		return reducedText + (index !== 0 ? ', ' : '') + fileName;
	};

	// Initialize a new error object or return an existing one
	var getError = function getError(errorId, message) {
		validatorErrors[errorId] = validatorErrors[errorId] || {
			files: [],
			hasFolderError: false,
			message: message
		};

		return validatorErrors[errorId];
	};

	// Logs the errors after validation has completed
	var flush = function flush() {
		(0, _keys2.default)(validatorErrors).forEach(function (id) {
			var error = validatorErrors[id];

			if (error.files.length) {
				var itemListStr = error.files.reduce(reduceMessage, '');
				logger(directoryInfo.path + ': ' + error.message + ' (' + error.files.length + ' file(s): ' + itemListStr + ')');
			}

			if (error.hasFolderError) {
				logger(directoryInfo.path + ': ' + error.message);
			}
		});
	};

	var addFile = function addFile(fileName, errorId, message) {
		getError(errorId, message).files.push(fileName);
		totalErrors.add(errorId, message);
	};

	var addFolder = function addFolder(folderPath, errorId, message) {
		getError(errorId, message).hasFolderError = true;
		totalErrors.add(errorId, message);
	};

	return {
		addFile: addFile,
		addFolder: addFolder,
		flush: flush
	};
};

var TotalErrorCounter = exports.TotalErrorCounter = function TotalErrorCounter() {
	var errors = {};

	// REPORTING
	var formatErrorCount = function formatErrorCount(error) {
		var message = error.message;

		// Decapitalize
		if (message.charAt(1) === message.charAt(1).toLowerCase()) {
			message = message.charAt(0).toLowerCase() + message.substr(1);
		}

		return message + ' (count: ' + error.count + ')';
	};

	var reduceMessage = function reduceMessage(reducedText, errorId, index) {
		return reducedText + (index !== 0 ? ', ' : '') + formatErrorCount(errors[errorId]);
	};

	var format = function format() {
		return (0, _keys2.default)(errors).reduce(reduceMessage, '');
	};

	// ADDING ERRORS
	var add = function add(errorId, message) {
		errors[errorId] = errors[errorId] || {
			count: 0,
			message: message
		};

		errors[errorId].count++;
	};

	// MISC

	// Get error count by type
	// If no type is specified, total error count is returned
	var count = function count(errorId) {
		if (errorId) {
			return errors[errorId] ? errors[errorId].count : 0;
		}

		return (0, _keys2.default)(errors).reduce(function (total, id) {
			return total + errors[id].count;
		}, 0);
	};

	// Return a single error
	var pickOne = function pickOne() {
		var id = (0, _keys2.default)(errors)[0];
		return {
			id: id,
			message: errors[id].message
		};
	};

	return {
		add: add,
		count: count,
		format: format,
		pickOne: pickOne,
		getErrors: function getErrors() {
			return errors;
		}
	};
