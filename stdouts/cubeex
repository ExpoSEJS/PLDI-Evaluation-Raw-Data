/***/ function(module, exports, __webpack_require__) {

	'use strict';
	// ECMAScript 6 symbols shim
	var global         = __webpack_require__(29)
	  , has            = __webpack_require__(44)
	  , DESCRIPTORS    = __webpack_require__(38)
	  , $export        = __webpack_require__(28)
