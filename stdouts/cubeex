	        _triggerDragStart: function _triggerDragStart(touch) {
	            if (touch) {

	                tapEvt = {
	                    target: dragEl,
	                    clientX: touch.clientX,
	                    clientY: touch.clientY
	                };

	                this._onDragStart(tapEvt, 'touch');
	            } else if (!this.nativeDraggable) {
	                this._onDragStart(tapEvt, true);
	            } else {
