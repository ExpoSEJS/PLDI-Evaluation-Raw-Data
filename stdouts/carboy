# Setup the handlebars-layouts plugin, plus the html5 boilerplate templates

fs = require('fs')
# ugly as heck - I need to patch the handlebars-layout helpers onto the grunt task's
# local version of handlebars, not my own. No better way I can see.
handlebars = require("../../grunt-compile-handlebars/node_modules/handlebars")
require("handlebars-layouts")(handlebars)

html5Path = path.join(__dirname, "..", "bower_components", "html5-boilerplate-template", "index.hbs-layout")
handlebars.registerPartial('html5', fs.readFileSync(html5Path, 'utf-8'))


module.exports = (grunt) ->

  grunt.loadNpmTasks("grunt-compile-handlebars")
  grunt.loadNpmTasks("grunt-contrib-connect")
  grunt.loadNpmTasks("grunt-contrib-copy")
  grunt.loadNpmTasks("grunt-contrib-clean")
  grunt.loadNpmTasks("grunt-contrib-sass")
  grunt.loadNpmTasks("grunt-contrib-watch")
  grunt.loadNpmTasks("grunt-webfont")
  grunt.loadNpmTasks("grunt-marathon-dns")

  defaults = 
    port: 3000
    public: "public"
    fonts: "fonts"
    fontsSrc: "fonts/src"
    images: "images"
    pages: "pages"
    data: "data"
    site: "site.json"
    styles: "styles"
    build: "build"

  grunt.config.set('carboy.port', grunt.config.get('carboy.port') or process.env.PORT or defaults.port)
  grunt.config.set('carboy.public', grunt.config.get('carboy.public') or defaults.public)
  grunt.config.set('carboy.fonts', grunt.config.get('carboy.fonts') or defaults.fonts)
  grunt.config.set('carboy.fontsSrc', grunt.config.get('carboy.fontsSrc') or defaults.fontsSrc)
  grunt.config.set('carboy.images', grunt.config.get('carboy.images') or defaults.images)
  grunt.config.set('carboy.pages', grunt.config.get('carboy.pages') or defaults.pages)
  grunt.config.set('carboy.data', grunt.config.get('carboy.data') or defaults.data)
  grunt.config.set('carboy.site', grunt.config.get('carboy.site') or defaults.site)
  grunt.config.set('carboy.styles', grunt.config.get('carboy.styles') or defaults.styles)
  grunt.config.set('carboy.build', grunt.config.get('carboy.build') or defaults.build)

  # Config

  # clean
  grunt.config.set("clean.build", "<%= carboy.build %>")

  # webfont
  grunt.config.set "webfont", 
    all:
      src: "<%= carboy.fontsSrc %>/**/*.svg"
      dest: "<%= carboy.build %>/assets/fonts"
      destCss: "<%= carboy.build %>/styles"
      options:
        htmlDemo: false
        relativeFontPath: '/assets/fonts/'
        syntax: 'bootstrap'
        stylesheet: 'sass'

  # sass
  grunt.config.set "sass",
    all:
      options:
        loadPath: [ path.join(__dirname, "..", "bower_components", "bootstrap-sass", "lib") ]
      src: "<%= carboy.styles %>/main.scss"
      dest: "<%= carboy.build %>/styles/main.css"

  # handlebars
  grunt.config.set "compile-handlebars",
    all:
      template: "<%= carboy.pages %>/**/*.hbs"
      templateData: "<%= carboy.data %>/**/*.json"
      output: "<%= carboy.build %>/**/*.html"
      globals: [ "<%= carboy.site %>" ]

  # assets
  grunt.config.set "copy",
    images:
      expand: true
      cwd: "<%= carboy.images %>"
      src: "**/*"
      dest: "<%= carboy.build %>/images"
    fonts:
      expand: true
      cwd: "<%= carboy.fonts %>"
      src: "*.{eot,woff,ttf}"
      dest: "<%= carboy.build %>/fonts"
    public:
      expand: true
      cwd: "<%= carboy.public %>"
      src: "**/*"
      dest: "<%= carboy.build %>"

  grunt.config.set "connect", 
    server:
      options:
        port: "<%= carboy.port %>"
        base: "<%= carboy.build %>"

  grunt.config.set "watch",
    options:
      livereload: true
    fonts:
      files: "<%= carboy.fontsSrc %>/**/*.svg"
      tasks: [ "webfont", "sass" ]
    styles:
      files: "<%= carboy.styles %>/**/*.scss"
      tasks: [ "sass" ]
    pages:
      files: "<%= carboy.pages %>/**/*.hbs"
      tasks: [ "compile-handlebars" ]
    assets:
      files: [ "<%= carboy.public %>**/*", "<%= carboy.images %>**/*"]
      tasks: [ "copy" ]


  grunt.registerTask "carboy", "carboy:serve"
  grunt.registerTask "carboy:serve", [ 
    "carboy:build"
    "marathon_dns"
    "connect"
    "watch"
  ]
  grunt.registerTask "carboy:build", [
    "clean"
    "webfont"
    "sass"
    "compile-handlebars"
    "copy"
  ]

